#profile.login
  %h3 Ienākt

  .info
    = form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|

      %p
        %div
          =f.label :email
        =f.email_field :email
      %p
        %div
          =f.label :password
        =f.password_field :password

      -if devise_mapping.rememberable?
        %div
          = f.check_box :remember_me
          = f.label :remember_me
      
      .actions
        = f.submit "Ienākt"
        
      .clear

